#Sample NFT Contract using ERC721 

// SPDX-License-Identifier:UNLICENSED

pragma solidity ^0.8.4;

import “https://github.com/OpenZeppelin/openzeppelin-contracts/contracts/access/Ownable.sol";
import “https://github.com/OpenZeppelin/openzeppelin-contracts/contracts/token/ERC721/ERC721.sol";

contract NeoaidERC721 is Ownable, ERC721(“Neoaid-NFT”, “NANFT”){
uint tokenId;

mapping(walletAddress=>tokenMetaData[]) public ownershipRecord;

struct metaDataToken{

uint tokenId;
uint timeStamp;
string tokenURI;
}

function mintToken(address recipient) onlyOwner public {
require(owner()!=recipient, “Recipient cannot be the owner of the contract”);
_safeMint(recipient, tokenId);
ownershipRecord[recipient].push(tokenMetaData(tokenId, block.timestamp, “https://ipfs.io/ipfs/QmbThsq3GKefUqw2fKiQo36pKSjwEGuJRc7JSFC2UjWJnz?filename=medicine.webp"));
tokenId = tokenId + 1;
}
}
